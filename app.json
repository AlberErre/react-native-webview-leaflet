{
  "expo": {
    "name": "react-native-webview-leaflet",
    "description": "This webview containing a clickable Leaflet map.",
    "slug": "react-native-webview-leaflet",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "assetExts": ["html"]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "react-native-webview-leaflet",
            "project": "react-native",
            "authToken": "970297191dc04411bfe95b43a00e33a28d06da933dae4b05aa09cd8933a4402d"
          }
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.reggie3.reactnativewebviewleaflet",
      "supportsTablet": true
    },
    "android": {
      "package": "com.reggie3.reactnativewebviewleaflet"
    }
  }
  
}
